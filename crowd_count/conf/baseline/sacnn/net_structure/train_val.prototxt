name: "CrowdCount"
layer {
  name: "crowd"
  type: "CrowdData"
  top: "image"
  top:"dmap"
  include {
    phase: TRAIN
  }
  transform_param {
    scale: 0.007843
    mean_value: 103.939
    mean_value: 116.779
    mean_value: 123.68    
  }

  image_data_param {
    source: "path-to-training-sample"
    root_folder:"path-to-training-root"
    shuffle: true
    batch_size: 1
    times_split: 0
    downsamp_times: 3
    neighbor_num: 5
    dmap_online:true
    ksize_param: 0.25
    sigma_param: 0.12
    scale_param: 2
    is_augment:false
  }
}

layer {
  name: "crowd"
  type: "CrowdData"
  top: "image"
  top:"dmap"
  include {
    phase: TEST
  }
  transform_param {
    scale: 0.007843
    mean_value: 103.939
    mean_value: 116.779
    mean_value: 123.68    
  }
  image_data_param {
    source: "path-to-validate-sample"
    root_folder:"path-to-validate-root"
    shuffle: true
    batch_size: 1
    times_split: 0
    downsamp_times: 3
    neighbor_num: 5
    dmap_online:true
    ksize_param: 0.25
